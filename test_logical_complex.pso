ALGORITHME TestLogicalComplex

VARIABLES
    a: ENTIER
    b: ENTIER
    c: ENTIER

DEBUT
    a <- 7
    b <- 3
    c <- 5

    // Nested NON
    SI NON NON (a > b) ALORS
        ECRIRE("Double NOT works correctly")
    FINSI

    // Complex: (a > b ET b > c) OU (a == c)
    SI ((a > b ET b > c) OU a == c) ALORS
        ECRIRE("First complex condition passed")
    FINSI

    // Complex: NON (a < b) ET (c > 0)
    SI (NON (a < b) ET c > 0) ALORS
        ECRIRE("Second complex condition passed")
    FINSI

    // Three-way AND
    SI (a > 0 ET b > 0 ET c > 0) ALORS
        ECRIRE("All three variables are positive")
    FINSI

    // Mix of AND and OR with proper precedence
    SI (a == 7 OU (b == 2 ET c == 5)) ALORS
        ECRIRE("Precedence test passed")
    FINSI

FIN
