ALGORITHME Test02Arithmetic
VARIABLES
    x: ENTIER
    y: ENTIER
    z: ENTIER
DEBUT
    x <- 20
    y <- 5

    z <- (x + y)
    ECRIRE("20 + 5 = ", z)

    z <- (x - y)
    ECRIRE("20 - 5 = ", z)

    z <- (x * y)
    ECRIRE("20 * 5 = ", z)

    z <- (x / y)
    ECRIRE("20 / 5 = ", z)

    z <- (x % y)
    ECRIRE("20 % 5 = ", z)
FIN
