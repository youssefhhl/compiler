ALGORITHME TestOperatorsLogiques

VARIABLES
    x: ENTIER
    y: ENTIER
    z: ENTIER

DEBUT
    x <- 5
    y <- 10
    z <- 15

    // Test ET (AND)
    SI (x > 0 ET y > 0) ALORS
        ECRIRE("x et y sont positifs")
    FINSI

    // Test OU (OR)
    SI (x < 0 OU y < 0) ALORS
        ECRIRE("x ou y est negatif")
    SINON
        ECRIRE("x et y sont tous deux positifs")
    FINSI

    // Test NON (NOT)
    SI NON (x == y) ALORS
        ECRIRE("x n'est pas egal a y")
    FINSI

    // Test combinaison complexe
    SI (x > 0 ET y > 0 ET z > 0) ALORS
        ECRIRE("Tous les nombres sont positifs")
    FINSI

    // Test avec OU
    SI (x == 5 OU y == 5) ALORS
        ECRIRE("x ou y est egal a 5")
    FINSI

    // Test NON avec comparaison
    SI NON (x > y) ALORS
        ECRIRE("x n'est pas superieur a y")
    FINSI

    // Test combinaison ET et OU
    SI ((x > 0 ET y > 0) OU z < 0) ALORS
        ECRIRE("Condition satisfaite")
    FINSI

FIN
